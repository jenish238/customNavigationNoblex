<aura:component implements="forceCommunity:availableForAllPageTypes" access="global">
    
    <aura:attribute name="iframeUrl" type="String" />
    <aura:attribute name="width" type="String" />
    <aura:attribute name="height" type="String" />
    <aura:attribute name="scrolling" type="String" />
    <aura:attribute name="frameBorder" type="String" />
    <aura:attribute name="style" type="String" />
    <aura:attribute name="sandbox" type="String" />

    <style>
    .holdstheiframe {
  		background: url('https://https://coeliacaustralia.my.salesforce-sites.com/resource/1601434694000/spinner') top center no-repeat;
	}
    </style>
    
    <div class="holdstheiframe">
    
    <iframe aura:id="formiframeaura" id="formiframeaura" src="{!v.iframeUrl}"
            width="{!v.width}"
            height="{!v.height}"
            style="{!v.style}"
            frameBorder="{!v.frameBorder}"
            scrolling="{!v.scrolling}"
            />     
    </div>
    
        
    
</aura:component>